{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fileIngest"
		},
		"Bypass_LocalSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Bypass_LocalSource'"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_FTP_Raw_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_FTP_Raw'"
		},
		"LS_SFTP_Raw_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SFTP_Raw'"
		},
		"LS_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL'"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stfileingest123.dfs.core.windows.net/"
		},
		"LS_FTP_Raw_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ftp.gnu.org"
		},
		"LS_FTP_Raw_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "anonymous"
		},
		"LS_SFTP_Raw_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "test.rebex.net"
		},
		"LS_SFTP_Raw_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "demo"
		},
		"LS_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlingest-server.database.windows.net"
		},
		"LS_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbFileIngest"
		},
		"LS_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "paawan-admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fileIngest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Bypass_LocalSource_connectionString": {
						"value": "[parameters('Bypass_LocalSource_connectionString')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_FTP_Raw_password": {
						"value": "[parameters('LS_FTP_Raw_password')]"
					},
					"LS_SFTP_Raw_password": {
						"value": "[parameters('LS_SFTP_Raw_password')]"
					},
					"LS_SQL_password": {
						"value": "[parameters('LS_SQL_password')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"LS_FTP_Raw_properties_typeProperties_host": {
						"value": "[parameters('LS_FTP_Raw_properties_typeProperties_host')]"
					},
					"LS_FTP_Raw_properties_typeProperties_userName": {
						"value": "[parameters('LS_FTP_Raw_properties_typeProperties_userName')]"
					},
					"LS_SFTP_Raw_properties_typeProperties_host": {
						"value": "[parameters('LS_SFTP_Raw_properties_typeProperties_host')]"
					},
					"LS_SFTP_Raw_properties_typeProperties_userName": {
						"value": "[parameters('LS_SFTP_Raw_properties_typeProperties_userName')]"
					},
					"LS_SQL_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_properties_typeProperties_server')]"
					},
					"LS_SQL_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_properties_typeProperties_database')]"
					},
					"LS_SQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}